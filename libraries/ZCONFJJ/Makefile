include ${HOME}/BSR3/DEFS_03

N  = libzconfjj

S  =	mod_orb_jj.f90           mod_symc.f90             mod_symt.f90             \
	mod_conf_jj.f90          mod_target_jj.f90        mod_channel_jj.f90       \
	mod_channels_jj.f90      mod_phys_orb_jj.f90      mod_boef_list.f90        \
	mod_det_jq.f90           cfp_jj.f90               cjkj.f90                 \
	EL_nljk.f90              Incode_cj.f90            Incode_mult.f90          \
	Jdef_Jp.f90              Jdef_nc.f90              Jdef_ne.f90              \
	Jparity.f90              jterm.f90                kappa.f90                \
	label_jj.f90             mj_value.f90             new_index.f90            \
	prepare_iort_jj.f90      print_conf_jj.f90        Incode_int_jj.f90        \
	read_conf_jj.f90         read_core_jj.f90         read_expn_jj.f90         \
	smu.f90                  test_cj.f90              gen_jj_states.f90        \
	coef_1conf.f90           coef_2conf_jj.f90        read_peel_orb.f90        \
	check_conf_jj.f90        read_iort_jj.f90

O = $(S:.f90=.o)
 
$(N).a : $(O)
	ar rv $(N).a $(O)
	cp $(N).a $(LIBDIR)
	rm *.o *.a
 
clean:
	rm -f *.o *.mod *.a
 
.SUFFIXES:
.SUFFIXES: .f90 .mod .o .f
 
.f90.o:
	$(FC) $(compL)   $<
 
